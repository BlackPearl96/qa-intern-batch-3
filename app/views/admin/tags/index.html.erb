<button class="btn btn-primary" data-toggle="modal" id="btn-default" data-target="#create-tag">Create tag</button>
<div id="create-tag" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-body">
        <%= render "admin/tags/form" %>
      </div>
    </div>
  </div>
</div>
<h2>All Tags</h2>
<table id="container_tags" class="table table-striped table-bordered text-center">
  <thead>
    <tr>
      <th>Id</th>
      <th>Tag Name</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <%= render @tags %>
  </tbody>
</table>
<div class= "modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" id="ajaxModal" >
  <div class="modal-dialog">
    <div id='content' class='modal-content'>
    </div>
  </div>
</div>
