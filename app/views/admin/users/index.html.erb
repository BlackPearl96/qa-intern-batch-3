<% provide :title, t(".user") %>
<button class="btn default" data-toggle="modal" id="btn-default" data-target="#abc">Create user</button>
<div id="abc" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-body">
        <%= render "admin/users/form" %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-danger" data-dismiss="modal" id="btn-close">Close</button>
      </div>
    </div>
  </div>
</div>
<h2 id="title-user">All users</h2>
<table id="table_id" class="table table-striped table-bordered" style="width: 100%">
  <thead>
    <tr class="header-user">
      <th>Id</th>
      <th>Username</th>
      <th>Fullname</th>
      <th>Email</th>
      <th>Role</th>
      <th>Activated</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <%= render @users %>
  </tbody>
</table>
